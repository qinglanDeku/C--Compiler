int matrixSum(int matrix[3][5][6]){
    int i = 0;
    int j = 0;
    int k = 0;
    int retval = 0;
    while(i < 3){
        j = 0;
        while(j < 5){
            k = 0;
            while(k < 6){
                retval = retval + matrix[i][j][k];
                k = k + 1;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    return retval;
}

int main(){
    int m[3][5][6];
    int x = 0;
    int y = 0;
    int z = 0;
    int p = 1;
    while(x< 3){
        y = 0;
        while(y < 5){
            z = 0;
            while(z < 6){
               matrix[x][y][z] = p;
               p = p+ 1;
                z = z + 1;
            }
            y= y + 1;
        }
        x = x + 1;
    }
    write(matrixSum(m));
    return 1;
}